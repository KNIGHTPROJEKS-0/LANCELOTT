{
  "name": "lancelott-firebase-backend",
  "version": "2.1.0",
  "description": "🛡️ LANCELOTT v2.1.0 Enhanced - AI-Powered Security Framework with Firebase Cloud Backend, LangChain AI Integration, 27+ Security Tools, and Unified API",
  "main": "functions/index.js",
  "scripts": {
    "build": "npm run build:functions",
    "build:functions": "cd functions && npm run build",
    "serve": "firebase emulators:start",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:hosting": "firebase deploy --only hosting",
    "deploy:firestore": "firebase deploy --only firestore",
    "logs": "firebase functions:log",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.8.0",
    "express": "^5.1.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "uuid": "^9.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "multer": "^2.0.2",
    "sharp": "^0.32.6",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/multer": "^1.4.11",
    "@types/node-cron": "^3.0.11",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.0",
    "nodemon": "^3.0.2",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16",
    "firebase-functions-test": "^3.1.1"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/KNIGHTPROJEKS-0/LANCELOTT.git"
  },
  "keywords": [
    "firebase",
    "security",
    "penetration-testing",
    "ai",
    "langchain",
    "fastapi",
    "cloud-functions",
    "serverless",
    "cybersecurity",
    "reconnaissance",
    "vulnerability-scanner",
    "security-framework"
  ],
  "author": "LANCELOTT Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KNIGHTPROJEKS-0/LANCELOTT/issues"
  },
  "homepage": "https://github.com/KNIGHTPROJEKS-0/LANCELOTT#readme",
  "firebase": {
    "project": "lancelott-z9dko",
    "functions": {
      "source": "functions",
      "runtime": "python311"
    },
    "hosting": {
      "public": "dist",
      "rewrites": [
        {
          "source": "/api/**",
          "function": "lancelott-api"
        }
      ]
    }
  },
  "workspaces": [
    "functions"
  ]
}
