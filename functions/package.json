{
  "name": "lancelott-firebase-functions",
  "version": "2.1.0",
  "description": "Firebase Cloud Functions for LANCELOTT Security Framework",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts"
  },
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.8.0",
    "express": "^5.1.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "uuid": "^9.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "multer": "^2.0.2",
    "sharp": "^0.32.6",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^3.0.8",
    "joi": "^17.11.0",
    "crypto-js": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/lodash": "^4.14.202",
    "@types/uuid": "^9.0.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/multer": "^1.4.11",
    "@types/node-cron": "^3.0.11",
    "@types/compression": "^1.7.5",
    "@types/crypto-js": "^4.2.1",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.0",
    "nodemon": "^3.0.2",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16",
    "firebase-functions-test": "^3.1.1"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/KNIGHTPROJEKS-0/LANCELOTT.git"
  },
  "keywords": [
    "firebase-functions",
    "serverless",
    "cloud-functions",
    "security",
    "api",
    "backend"
  ],
  "author": "LANCELOTT Team",
  "license": "MIT"
}